}

.guide-top-img {
  width: 250rpx;
  height: 250rpx;
  animation: rotate 12s;
  /* background: rosybrown; */
}

@keyframes rotate {
  0% {
    width: 100rpx;
    height: 100rpx;
    /* margin-left: 0rpx; */
    opacity: 0.0;
  }

  25% {
    width: 250rpx;
    height: 250rpx;
    margin-top:0rpx;
    margin-left: 0rpx;
    opacity: 1;
    transform: rotateY(1440deg);
  }
  50%{
  width: 250rpx;
    height: 250rpx;
        margin-top:0rpx;
        /* margin-left: 0rpx; */
    opacity: 1;
       transform: rotateY(1440deg);
  }
  70%{
    width: 100rpx;
    height: 100rpx;
        margin-top:-50rpx;
        /* margin-left: -10rpx; */
    opacity: 1;
       transform: rotateY(1440deg);
  }
  100%{
        margin-top:-50rpx;
        /* margin-left: -10rpx; */
     width: 100rpx;
    height: 100rpx;
    opacity: 1;
       transform: rotateY(1440deg);
  }
}

.guide-middle {
  width: 100%;
  height: 200rpx;
  /* background: coral; */
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.guide-middle-left {
  width: 250rpx;
  height: 80rpx;
  opacity: 0.0;
  /* background: aliceblue; */
  animation: guideleft 12s linear ;
}

.guide-middle-rigth {
  width: 250rpx;
  height: 80rpx;
  opacity: 0.0;
  /* background: aliceblue; */
  animation: guiderigth 12s linear ;
}

@keyframes guideleft {
  0% {
    opacity: 0.0;
  }

  25% {
    opacity: 0.0;
  }

  50% {
    opacity: 1;
    margin-top: 0rpx;
    margin-right: 0rpx;
    width: 250rpx;
    height: 80rpx;
  }

  75% {
    opacity: 1;
    width: 180rpx;
    height: 60rpx;
    margin-top: -480rpx;
    margin-right: -120rpx;
  }

  100% {
    opacity: 0.0;
    width: 180rpx;
    height: 60rpx;
     margin-top: -480rpx;
    margin-rigth: -120rpx;
  }
}

@keyframes guiderigth {
  0% {
    opacity: 0.0;
  }

  25% {
    opacity: 0.0;
  }

  50% {
    opacity: 1;
    width: 250rpx;
    height: 80rpx;
    margin-top: 0rpx;
    margin-left: 0rpx;
  }

  75% {
    opacity: 1;
    width: 180rpx;
    height: 60rpx;
    margin-top: -480rpx;
    margin-left: -120rpx;
  }

  100% {
    opacity: 0;
    width: 180rpx;
    height: 60rpx;
     margin-top: -480rpx;
    margin-left: -120rpx;
  }
}

