page{
  background-color: #eee;
}
.img{
  height: 350rpx;
  width: 100%;
}
.box{
  width: 100%;
  border-bottom: 1rpx solid rgb(156, 156, 156);
  background-color: white;
  margin-top: -7rpx;
}
.box-font{
  width: 80%;
  margin: 0 auto;
  text-align: center;
  padding-top: 30rpx;
}
.box-font2{
  width: 80%;
  margin: 0 auto;
  text-align: center;
  padding-top: 10rpx;
  font-size: 30rpx;
  color: rgb(187, 187, 187);
  padding-bottom: 30rpx;
  border-bottom: 1rpx solid rgb(216, 216, 216);
}
.box-font3{
  width: 80%;
  margin: 0 auto;
  display: flex;
  align-items: center;
  height: 150rpx;
}
.box-font3-a1{
  width: 70%;
  height: 120rpx;
  border-right: 1rpx solid rgba(223, 223, 223, 0.404);
  display: flex;
  align-items: center;
}
.icon-icon-dingwei{
  font-size: 30rpx;
  width: 80rpx;
}
.box-font3-a1-b{
  font-size: 27rpx;
  margin-right: 10rpx;
}
.box-font3-a2{
  width: 30%;
  height: 120rpx;
  display: flex;
  align-items: center;
}
.box-font3-a2-box{
  width: 50%;
  height: 120rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}
.icon-daohang{
  font-size: 45rpx;
  color: rgb(255, 119, 65);
}
.icon-weibiaoti-{
  font-size: 47rpx;
  color: rgb(255, 119, 65);
}
.box-font3-a2-box-font{
  font-size: 30rpx;
}
/* 相关攻略 */

.main-bottom {
  width: 96%;
  margin: 0 auto;
  overflow: hidden;
  background: rgb(255, 255, 255);
  border-radius: 20rpx;
}
.img2{
  width: 100%;
}
.main {
  margin-top: 20rpx;
  background-color: white;
}

.main-top {
  display: flex;
  justify-content: space-between;
  padding: 20rpx 0 20rpx 0;
}

.main-top-left {
  margin-left: 40rpx;
  font-weight: 600;
}
.main-bottom-font{
  word-break:break-all; 
  padding-bottom: 50rpx;
  
}
/* 评论 */

.comment{
  width: 100%;
  background-color: white;
  margin-top: 20rpx;
  padding-bottom: 25rpx;
}
.comment-top{
  width: 94%;
  margin: 0 auto;
  padding-top: 15rpx;
  padding-bottom: 20rpx;
  border-bottom: 1rpx solid rgb(201, 201, 201);
}
.comment-box{
  width: 94%;
  margin: 15px auto 0 auto;
 
  box-shadow: 1px 3px 3px 1px #888;
}
.comment-box-top{
  border-bottom: 1rpx solid rgb(223, 223, 223);
  padding-top: 15rpx;
  padding-bottom: 15rpx;
  display: flex;
}
.comment-box-top-left{
  border-radius: 50%;
  background-color: rgb(56, 165, 255);
  width: 70rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  color: white;
  font-size: 30rpx;
  font-weight: 600;
  margin-left:15rpx; 
}
.comment-box-top-right{
  height: 70rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin-left:15rpx; 
}
.comment-box-top-right-top{
  font-size: 30rpx;
}
.comment-box-top-right-bottom{
  font-size: 25rpx;
}
.comment-box-bottom{
  word-break: break-word;
  width: 96%;
  margin: 0 auto;
  font-size: 30rpx;
}
.end{
  padding-top: 20rpx;
  padding-bottom: 100rpx;
  margin: 0 auto;
  color: rgb(206, 206, 206);
  text-align: center;
}